<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="intern.line.me.kyotoaclient">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activity.AuthActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RoomListActivity">
        </activity>
        <activity android:name=".MessageActivity"
            android:parentActivityName=".RoomListActivity">
        </activity>
        <activity android:name=".activity.RoomCreateActivity">
        </activity>
        <activity android:name=".activity.LoginedActivity">
        </activity>
        <activity android:name=".activity.UserListActivity">

        </activity>
        <activity android:name=".activity.ChangeMyProfileActivity">
        </activity>
        <activity android:name=".activity.GetUserProfileActivity">
        </activity>
    </application>

</manifest>
